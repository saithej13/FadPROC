<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/ac"
        android:padding="10dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:text="000"
            android:id="@+id/txtccode"
            android:gravity="center"
            android:textStyle="bold"
            android:textColor="@color/colourPrimary"
            android:textSize="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
        <TextView
            android:text="slno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtslno"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="center"
            android:visibility="invisible"
            android:layout_below="@id/txtcname"
            android:layout_marginLeft="10dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"/>

        <TextView
            android:text="Text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtcname"
            android:textSize="17sp"
            android:maxLength="30"
            android:textStyle="bold"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@id/txtdate"
            android:layout_toRightOf="@id/txtccode" />
        <TextView
            android:text="Date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtdate"
            android:textSize="17sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@id/txtshift"
            />
        <TextView
            android:text="shift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtshift"
            android:textSize="17sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="left"
            android:layout_marginLeft="5dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <TextView
            android:text="mtype"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtmtype"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="center"
            android:textStyle="bold"
            android:layout_below="@id/txtcname"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/txtccode" />
        <TextView
            android:text="QTY : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textqty"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="center"
            android:layout_below="@id/txtcname"
            android:layout_marginLeft="10dp"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/txtmtype"

            /> <TextView
        android:text="0.0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtqty"
        android:textSize="15sp"
        android:maxLength="30"
        android:textColor="@color/colourPrimary"
        android:gravity="center"
        android:layout_below="@id/txtcname"
        android:layout_marginLeft="2dp"
        android:textStyle="bold"
        android:layout_alignParentTop="false"
        android:layout_toRightOf="@id/textqty"
        android:layout_marginStart="2dp"
        android:layout_toEndOf="@id/textqty" />
        <TextView
            android:text="fat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textfat"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_below="@id/txtcname"
            android:layout_marginLeft="10dp"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/txtqty"
            />
        <TextView
            android:text="0.0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtfat"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_below="@id/txtcname"
            android:layout_marginLeft="2dp"
            android:textStyle="bold"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/textfat"
            />
        <TextView
            android:text="Snf : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textsnf"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_below="@id/txtcname"
            android:layout_marginLeft="10dp"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/txtfat"
            />
        <TextView
            android:text="0.0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtsnf"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_below="@id/txtcname"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="5dp"
            android:textStyle="bold"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/textsnf"
            />
        <TextView
            android:text="Rate : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textrate"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_marginLeft="10dp"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/txtccode"
            android:layout_below="@id/txtmtype"
            />
        <TextView
            android:text="0.0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtrate"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_marginLeft="2dp"
            android:textStyle="bold"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/textrate"
            android:layout_below="@id/textqty"
            />
        <TextView
            android:text="Amount : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textamt"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_marginLeft="10dp"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/txtrate"
            android:layout_below="@id/txtqty"
            />
        <TextView
            android:text="0.0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtamt"
            android:textSize="15sp"
            android:maxLength="30"
            android:textColor="@color/colourPrimary"
            android:gravity="start"
            android:layout_marginLeft="2dp"
            android:textStyle="bold"
            android:layout_alignParentTop="false"
            android:layout_toRightOf="@id/textamt"
            android:layout_below="@id/txtfat"
            />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>